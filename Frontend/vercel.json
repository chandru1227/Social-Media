{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "MY_ENV_VAR": "value"
    },
    "functions": {
      "api/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  